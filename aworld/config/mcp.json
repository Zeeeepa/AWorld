{
  "mcpServers": {
    "aworld": {
      "url": "http://localhost:2000/sse"
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ]
    },
    "amap-amap-sse": {
      "url": "https://mcp.amap.com/sse?key=YOUR_API_KEY",
      "timeout": 5.0,
      "sse_read_timeout": 300.0
    },
    "tavily-mcp": {
      "command": "npx",
      "args": ["-y", "tavily-mcp@0.1.2"],
      "env": {
        "TAVILY_API_KEY": "YOUR_API_KEY"
      }
    }
  } 
}