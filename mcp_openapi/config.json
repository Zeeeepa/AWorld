{
  "mcpServers": {
    "memory": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ]
    },
    "amap-amap-sse": {
      "type": "sse",
      "url": "https://mcp.amap.com/sse?key=${AMAP_AMAP_SSE_KEY}",
      "timeout": 5.0,
      "sse_read_timeout": 300.0
    },
    "tavily-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "tavily-mcp@0.1.2"
      ],
      "env": {
        "TAVILY_API_KEY": "${TAVILY_API_KEY}"
      }
    },
    "e2b-server": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@e2b/mcp-server"
      ],
      "env": {
        "E2B_API_KEY": "${E2B_API_KEY}",
         "SESSION_REQUEST_CONNECT_TIMEOUT": "120"
      }
    },
    "terminal-controller": {
      "type": "stdio",
      "command": "python",
      "args": [
        "-m",
        "terminal_controller"
      ]
    },
    "calculator": {
      "type": "stdio",
      "command": "python",
      "args": [
        "-m",
        "mcp_server_calculator"
      ]
    },
    "excel": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "--yes",
        "@negokaz/excel-mcp-server"
      ],
      "env": {
        "EXCEL_MCP_PAGING_CELLS_LIMIT": "4000"
      }
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@executeautomation/playwright-mcp-server"
      ]
    },
    "google-search": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@adenot/mcp-google-search"
      ],
      "env": {
        "GOOGLE_API_KEY": "${GOOGLE_API_KEY}",
         "GOOGLE_SEARCH_ENGINE_ID": "${GOOGLE_SEARCH_ENGINE_ID}"
      }
    },
    "ms-playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--no-sandbox",
        "--headless"
      ],
      "env": {
        "PLAYWRIGHT_TIMEOUT": "120000",
        "SESSION_REQUEST_CONNECT_TIMEOUT": "120"
      }
    },
    "audio_server": {
      "type": "stdio",
      "command": "python",
      "args": [
        "-m",
        "mcp_servers.audio_server"
      ]
    },
    "image_server": {
      "type": "stdio",
      "command": "python",
      "args": [
        "-m",
        "mcp_servers.image_server"
      ]
    },
    "youtube_server": {
      "type": "stdio",
      "command": "python",
      "args": [
        "-m",
        "mcp_servers.youtube_server"
      ]
    },
    "video_server": {
      "type": "stdio",
      "command": "python",
      "args": [
        "-m",
        "mcp_servers.video_server"
      ]
    },
    "search_server": {
      "type": "stdio",
      "command": "python",
      "args": [
        "-m",
        "mcp_servers.search_server"
      ]
    },
    "download_server": {
      "type": "stdio",
      "command": "python",
      "args": [
        "-m",
        "mcp_servers.download_server"
      ]
    },
    "browser_server": {
      "type": "stdio",
      "command": "python",
      "args": [
        "-m",
        "mcp_servers.browser_server"
      ]
    },
    "reasoning_server": {
      "type": "stdio",
      "command": "python",
      "args": [
        "-m",
        "mcp_servers.reasoning_server"
      ]
    },
    "document_server": {
      "type": "stdio",
      "command": "python",
      "args": [
        "-m",
        "mcp_servers.document_server"
      ]
    },
    "filesystem": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${FILESYSTEM_SERVER_WORKDIR}"
      ]
    }
  }
}